use dbinfox;
create table tb_usuario(
iduser int primary key,
usuario varchar(50) not null,
fone varchar(15) not null,
login varchar(50) not null unique,
senha varchar (50) not null,
perfil varchar (50) not null
); 
describe tb_usuario;

